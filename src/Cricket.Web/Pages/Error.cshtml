@page
@model ErrorModel
@{
    ViewData["Title"] = "Error";
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <meta name="theme-color" content="#000" />
    <base href="/" />
    <link rel="icon" href="/images/favicon.ico" type="image/x-icon" />
    <link rel="shortcut icon" href="/images/favicon.ico" type="image/x-icon" />
    <title>ICC</title>
    <style type="text/css">
        @@font-face { font-family: DynamicFont; font-style: normal; font-weight: 400; src: local('DynamicFont Regular'),local('DynamicFont-Regular'),url(/static/media/6aez4K2oVqwIvtU2Hw.7e006e4d.woff2) format('woff2'); unicode-range: U+0000-00FF,U+0131,U+0152-0153,U+02BB-02BC,U+02C6,U+02DA,U+02DC,U+2000-206F,U+2074,U+20AC,U+2122,U+2191,U+2193,U+2212,U+2215,U+FEFF,U+FFFD }
        body { background-color: #eee; color: #212529; font-family: DynamicFont, Arial, Helvetica, sans-serif !important; font-size: 16px; font-weight: 400; line-height: 1.5; }
        h1, h5 { color: #253746; font-size: 2.5rem; font-weight: 500; line-height: 1.2; margin-top: 0; margin-bottom: 0.5rem; }
        h5 { font-size: 1.25rem; }
        hr { margin-top: 1rem; margin-bottom: 1rem; border: 0; border-top: 1px solid rgba(0, 0, 0, 0.1); }
        #root { -ms-flex-direction: row; flex-direction: row; height: 100%; min-height: 10vh; }
        .text-danger { color: #ce1126; }
        .container-content-group { margin: 1em; }
        .container-contents { background-color: #fff; border-radius: .4em; padding: 1.2em; }
        .contents a.btn, .contents a.btn:link, .contents a.btn:active, .contents a.btn:focus, .contents a.btn:hover, .contents a.btn:visited { color: #fff !important; }
        .btn { display: inline-block; text-align: center; vertical-align: middle; border: 1px solid transparent; padding: 0.375rem 0.75rem; font-size: 1rem; line-height: 1.5; border-radius: 0.25rem; text-decoration: none; }
        .btn-secondary, .btn-secondary:link { background-color: #6c757d; border-color: #6c757d; color: #fff; }
        .btn-secondary:hover, .btn-secondary:focus, .btn-secondary:active { background-color: #5a6268; border-color: #5a6268; color: #fff; }
    </style>
</head>
<body>
    <div id="root">
        <main role="main" class="container-content-group">
            <div class="container-contents">
                <div class="text-left">
                    <h1>An error occurred</h1>
                    <h5 class="text-danger">@(Model.ShowException ? Model.Error.Message : Model.HttpStatusMessage)</h5>
                    <hr />
                    <div class="contents">
                        <span class="text-muted">
                            If you see this message, something has gone wrong. You can try returning to home screen to try again.
                            <br />
                            If you repeatedly see this message after several attempts, please contact one of the Administrators of this site to get help.
                        </span>
                        <hr />
                        <a class="btn btn-secondary" name="Home" title="Click to navigate back home" asp-page="/Index">&lt; &lt; Home</a>
                    </div>
                </div>
            </div>
        </main>
    </div>
</body>
</html>